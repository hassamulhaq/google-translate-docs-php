{
    "name": "hassamulhaq/google-translate-docs-php",
    "description": "Translate docs file using Google Cloud Translation API",
    "type": "project",
    "require": {
        "php": "^7.4|^8.2",
        "vlucas/phpdotenv": "^5.6",
        "google/cloud-translate": "^1.18",
        "ext-json": "*",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Hassamulhaq\\GoogleTranslateDocsPhp\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t / router.php"
    },
    "authors": [
        {
            "name": "hassamulhaq"
        }
    ]
}
